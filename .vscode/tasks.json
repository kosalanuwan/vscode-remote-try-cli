{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "type": "shell",
            "command": "dotnet build ${workspaceFolder}/test-project/aspnetapp.csproj",
            "problemMatcher": "$tsc"
        },
        {
            "label": "publish",
            "type": "shell",
            "command": "dotnet publish ${workspaceFolder}/test-project/aspnetapp.csproj",
            "problemMatcher": "$tsc"
        },
        {
            "label": "watch",
            "type": "shell",
            "command": "dotnet watch -p ${workspaceFolder}/test-project/aspnetapp.csproj run",
            "problemMatcher": "$tsc"
        }
    ]
}